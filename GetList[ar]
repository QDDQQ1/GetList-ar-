        "iid": str(random.randint(1, 10 ** 19)),
        "channel": "googleplay",
        "device_type": "RMX3511",
        "language": "ar",
        "host_abi": "arm64-v8a",
        "locale": "ar",
        "resolution": "1080*2236",
        "openudid": str(binascii.hexlify(os.urandom(8)).decode()),
        "update_version_code": "330802",
        "ac2": "lte",
        "cdid": str(uuid.uuid4()),
